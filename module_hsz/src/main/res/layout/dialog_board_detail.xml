<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="1000px"
                                     android:orientation="vertical"
              android:background="@drawable/shape_white_corner10"
              android:layout_height="890px">
    <!--<ImageView-->
        <!--android:src="@drawable/huli_table"-->
        <!--android:layout_width="1400px"-->
        <!--android:layout_height="860px"/>-->
    <com.zhy.autolayout.AutoRelativeLayout
        android:layout_width="1000px"
        android:layout_height="70px">
        <TextView
            android:layout_centerInParent="true"
            android:textSize="28px"
            android:textColor="@color/public_text_60"
            android:text="护理白板"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <com.zhy.autolayout.AutoRelativeLayout
            android:id="@+id/dialog_board_detail_close_btn"
            android:layout_alignParentRight="true"
            android:layout_width="70px"
            android:layout_height="70px">
            <ImageView
                android:layout_centerInParent="true"
                android:src="@drawable/icon_close"
                android:layout_width="37px"
                android:layout_height="37px"/>
        </com.zhy.autolayout.AutoRelativeLayout>
    </com.zhy.autolayout.AutoRelativeLayout>
    <com.ekz.ctt.eckctt.app.widget.MySmartTable
        android:layout_gravity="center_horizontal"
        android:id="@+id/dialog_board_detail_smarttable"
        android:layout_width="900px"
        android:layout_height="820px" />
</com.zhy.autolayout.AutoLinearLayout>